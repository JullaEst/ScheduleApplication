package Credentials;

/**
 * Credentials for login information
 */
public class Credentials {
    private static final String userNameTest = "test";
    private static final String passwordTest = "test";
    private static final String userNameAdmin = "admin";
    private static final String passwordAdmin = "admin";

    public static boolean validate(String usernameInput, String passwordInput){
        return(userNameTest.equals(usernameInput) && passwordTest.equals(passwordInput)) || (userNameAdmin.equals(usernameInput) && passwordAdmin.equals(passwordInput));
    }

}
